generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_TEST_URL")
  referentialIntegrity = "prisma"
}

model Shit {
  id     Int    @id @default(autoincrement())
  rating Int
  name   String @unique
}
